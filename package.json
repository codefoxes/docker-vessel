{
	"name": "vessel",
	"version": "1.0.0",
	"description": "Docker vessel",
	"main": "main.js",
	"scripts": {
		"start": "npm run parcel && electron .",
		"electron": "electron .",
		"parcel": "rimraf dist && parcel build ./src/windows/*.html --public-url ./ --no-minify --target electron",
		"web": "parcel ./src/windows/index.html -d build --open"
	},
	"repository": "https://github.com/codefoxes/vessel",
	"keywords": [
		"Electron",
		"docker"
	],
	"author": "Codefoxes",
	"license": "GPL3",
	"devDependencies": {
		"@vue/component-compiler-utils": "^2.5.2",
		"electron": "^4.0.4",
		"eslint": "^5.14.0",
		"eslint-config-airbnb-standard": "^3.0.1",
		"eslint-plugin-vue": "^5.2.1",
		"node-sass": "^4.11.0",
		"parcel-bundler": "^1.11.0",
		"rimraf": "^2.6.3",
		"vue-template-compiler": "^2.6.6"
	},
	"dependencies": {
		"graceful-fs": "^4.1.15",
		"grid-kit": "git://github.com/codefoxes/grid-kit.git#kart",
		"js-yaml": "^3.12.1",
		"requirejs": "^2.3.6",
		"vue": "^2.6.6",
		"vue-codemirror": "^4.0.6",
		"vue-hot-reload-api": "^2.3.2",
		"vuex": "^3.1.0"
	}
}
